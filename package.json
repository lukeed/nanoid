{
  "version": "1.0.0",
  "name": "@lukeed/nanoid",
  "repository": "lukeed/nanoid",
  "description": "A tiny (141 bytes), secure URL-friendly unique string ID generator",
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "browser": "dist/nanoid.esm.js",
  "unpkg": "dist/nanoid.min.js",
  "module": "dist/nanoid.mjs",
  "main": "dist/nanoid.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bundt --main --module && bundt src/browser.js --unpkg --browser",
    "pretest": "npm run build",
    "test": "tape test/*.js | tap-spec"
  },
  "devDependencies": {
    "bundt": "^0.3.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
